package com.qa.java.basics.classes;

public class Books {
	public String name;
	public double size;
	public String color;
	public String author;
	public int pages;
	
	
	//Constructor
	public Books(String name, String author) {
		this.name = name;
		this.author = author;
	}


	public Books() {
		// TODO Auto-generated constructor stub
	}


	
}
