package com.qa.java;

public class Calculator {

	public static int add(int i, int j, Object ans) {

		ans = i+j;
		System.out.println("ans");
		return j;

	}
	
	public static void minus(int i, int j, Object ans) {
		ans = i-j;
		System.out.println("ans");

	}

	public static void multiply(int i, int j, Object ans) {
		ans = i*j;
		System.out.println("ans");

	}
	public static int divide(int i, int j, Object ans) {
		ans = i/j;
		System.out.println("ans");
		return j;
	}
}


