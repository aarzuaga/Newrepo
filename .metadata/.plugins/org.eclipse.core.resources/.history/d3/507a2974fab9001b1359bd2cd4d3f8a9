package com.qa.java.basics.classes;

public class Motorcycles extends Vehicles {
	
	public String name; 
	public int year; 
	public String fuelType;
	public String color;
	public int maxSpeed;
	public int price;
	public int numWheels;
	public String allTerrain;
	
	public Motorcycles(int year, String name, String color, int price) {
		super(year, name, color, price);
	}
	public void start() {
		System.out.println("Starting Motorcycles");

	}
	@Override
	public void accelerate() {
		System.out.println("moving forward");

		
	}
	@Override
	public void reverse() {
		System.out.println("moving backwards");
		
	}
	@Override
	public void park() {
		System.out.println("shifting gear to park");

		
	}
	@Override
	public void turnLeft() {
		// TODO Auto-generated method stub
		
	}
	@Override
	public void turnRight() {
		// TODO Auto-generated method stub
		
	}
	@Override
	public void turnOff() {
		// TODO Auto-generated method stub
		
	}
}
