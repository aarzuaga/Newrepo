package com.qa.java.basics.classes;

public class Cars extends Vehicles {

	public Cars(int year, String name, String color, int price) {
		super(year, name, color, price);

	}

	public String name;
	public int year;
	public String fuelType;
	public String color;
	public int maxSpeed;
	public int price;

	@Override
	public void start() {
		System.out.println("Starting Cars");

	}

	@Override
	public void accelerate() {
		System.out.println("moving forward");
	}

	@Override
	public void reverse() {
		System.out.println("moving backwards");
	}

	@Override
	public void park() {
		System.out.println("shifting gear to park");

	}

	@Override
	public void turnLeft() {
		System.out.println("turning left");

	}

	@Override
	public void turnRight() {
		System.out.println("turning right");

	}

	@Override
	public void turnOff() {
		System.out.println("turn the engine off");

	}

}
